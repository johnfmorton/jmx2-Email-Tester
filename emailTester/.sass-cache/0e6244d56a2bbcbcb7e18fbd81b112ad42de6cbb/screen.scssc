 * In this file you should write your main styles. (or centralize your imports)
 * Import this file using the following HTML or equivalent:
 * <link href="/stylesheets/screen.css" media="screen, projection" rel="stylesheet" type="text/css" /> */

@import "compass/reset";
@import "compass/css3";
@import "bootstrap";

.navbar-inner {
	border-radius: 0;
}

#footer p {
	margin: 20px;
	font-size: 10px;
}

#htmlForEmail {
	width: 98%;
	padding: 1%;
	margin: 0 0 20px 0;
	min-height: 650px;

	font-family: "Courier New", Courier, "Lucida Sans Typewriter", "Lucida Typewriter", monospace;
	background-color: #272822;
	color: #e6d979;
}

#userManagement {
	margin-bottom: 0;
}

#userManagementContent{
	padding: 10px;
	background-color: #FFF;
	min-height: 130px;
	@include border-radius(4px);
}

#saveSubject {
	margin: -10px 0 0 5px;
}

body
{
     background-image: url(data:image/png;base64,);
     padding: 0 0 30px 0;
}

.alertify-log.alertify-log-notice {
	background-color: #666;
	background-color: rgba(102, 102, 102, 0.8);
}

.recipient {
	display: inline-block;
	padding: 4px 8px;
	margin-bottom: 0;
	font-size: 14px;
	line-height: 20px;
	color: #999;
	text-align: center;
	text-shadow: 0 1px 1px rgba(255, 255, 255, 0.75);
	vertical-align: middle;
	.check {
		margin: -1px 5px 0 0;
	}
	border: 1px solid #ccc;
	@include border-radius(4px);
	margin: 3px;
}

.recipient .check:checked + span {
	color: #000;
}

#deleteusers .recipient:hover {
	cursor:pointer;
	color: #ff3300;
	border-color: #ff3300;
}

#deleteusers {
	.icon-trash {
		margin: 1px 5px 0 -2px;
	}
}

.errorheadline {
	color: red;
	font-weight: bold;
	font-size: 120%;
	line-height: 140%;
	margin-bottom: 10px;
}
.successheadline {
	color: #3B7808;
	font-weight: bold;
	font-size: 120%;
	line-height: 140%;
	margin-bottom: 10px;
}:
